{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.26",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aginix/nestjs-firebase-admin": "^1.1.0",
    "@nestjs-addons/in-memory-db": "^1.8.10",
    "@nestjs/common": "^7.0.5",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.5",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.5",
    "@nestjs/platform-socket.io": "^7.0.5",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/websockets": "^7.0.5",
    "@types/cookie-parser": "^1.4.2",
    "aws-sdk": "^2.745.0",
    "canvas": "^2.6.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "face-api.js": "^0.22.2",
    "faker": "^4.1.0",
    "generate-pincode": "^5.0.3",
    "in-app-purchase": "^1.11.4",
    "is-image": "^3.0.0",
    "lorem-ipsum": "^2.0.3",
    "moment": "^2.26.0",
    "mongoose": "^5.9.25",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "random-id": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sharp": "^0.26.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^7.0.13",
    "@types/webgl2": "0.0.5",
    "@types/express": "^4.17.3",
    "@types/jest": "^24.9.1",
    "@types/mongoose": "^5.7.32",
    "@types/node": "^13.9.4",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/sharp": "^0.26.0",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
